{
    "extends": "@vue/tsconfig/tsconfig.web.json",
    "include": [
        "env.d.ts",
        "src/**/*",
        "src/**/*.vue"
    ],
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./src/*"
            ]
        },
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ES2022"
        ]
    },
    "references": [
        {
            "path": "./tsconfig.config.json"
        }
    ]
}
